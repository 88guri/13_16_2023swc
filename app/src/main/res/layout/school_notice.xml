<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="430dp"
        android:layout_height="769dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/titleTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="학교 공지 제목"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.069"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/line69"
            android:layout_width="385dp"
            android:layout_height="21dp"
            android:layout_marginTop="8dp"
            android:elevation="0dp"
            android:src="@drawable/line69"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/titleTextView" />

        <ImageView
            android:id="@+id/userikon"
            android:layout_width="41dp"
            android:layout_height="34dp"
            android:layout_marginStart="24dp"
            android:elevation="0dp"
            android:src="@drawable/userikon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <TextView
            android:id="@+id/authorTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="작성자 이름"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@id/userikon"
            app:layout_constraintTop_toTopOf="@id/userikon"
            tools:layout_editor_absoluteX="64dp"
            tools:layout_editor_absoluteY="74dp" />

        <TextView
            android:id="@+id/dateTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:text="작성일"
            android:textSize="10sp"
            app:layout_constraintStart_toStartOf="@id/authorTextView"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <TextView
            android:id="@+id/viewsTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="48dp"
            android:layout_marginEnd="256dp"
            android:text="조회수"
            android:textSize="10sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/line69" />

        <ImageView
            android:id="@+id/line70"
            android:layout_width="391dp"
            android:layout_height="24dp"
            android:layout_marginTop="32dp"
            android:elevation="0dp"
            android:src="@drawable/ine70"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <ImageView
            android:id="@+id/mainrec"
            android:layout_width="539dp"
            android:layout_height="479dp"
            android:layout_marginTop="8dp"
            android:elevation="0dp"
            android:src="@drawable/mainrec"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.577"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/line70" />

        <ImageView
            android:id="@+id/line71"
            android:layout_width="434dp"
            android:layout_height="116dp"
            android:layout_marginTop="492dp"
            android:elevation="0dp"
            android:src="@drawable/line71"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.666"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <ImageView
            android:id="@+id/vector"
            android:layout_width="67dp"
            android:layout_height="61dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="568dp"
            android:elevation="0dp"
            android:src="@drawable/vector"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <ImageView
            android:id="@+id/line72"
            android:layout_width="48dp"
            android:layout_height="96dp"
            android:layout_marginStart="64dp"
            android:layout_marginTop="552dp"
            android:elevation="0dp"
            android:src="@drawable/line72"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <ImageView
            android:id="@+id/school"
            android:layout_width="87dp"
            android:layout_height="85dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="556dp"
            android:elevation="0dp"
            android:src="@drawable/school"
            app:layout_constraintStart_toEndOf="@id/vector"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <ImageView
            android:id="@+id/line73"
            android:layout_width="84dp"
            android:layout_height="113dp"
            android:layout_marginTop="552dp"
            android:layout_marginEnd="80dp"
            android:elevation="0dp"
            android:src="@drawable/line73"
            app:layout_constraintEnd_toStartOf="@id/biguserikon"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <ImageView
            android:id="@+id/imac"
            android:layout_width="94dp"
            android:layout_height="96dp"
            android:layout_marginTop="552dp"
            android:layout_marginEnd="136dp"
            android:elevation="0dp"
            android:src="@drawable/imac"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/line69" />

        <ImageView
            android:id="@+id/line74"
            android:layout_width="81dp"
            android:layout_height="114dp"
            android:layout_marginStart="268dp"
            android:layout_marginTop="8dp"
            android:elevation="0dp"
            android:src="@drawable/line74"
            app:layout_constraintStart_toStartOf="@id/line71"
            app:layout_constraintTop_toBottomOf="@id/mainrec" />

        <ImageView
            android:id="@+id/biguserikon"
            android:layout_width="96dp"
            android:layout_height="89dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="556dp"
            android:elevation="0dp"
            android:src="@drawable/biguserikon"
            app:layout_constraintStart_toStartOf="@id/line74"
            app:layout_constraintTop_toBottomOf="@id/line69" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>



